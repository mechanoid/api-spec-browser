{
  "name": "api-spec-browser",
  "description": "simple API viewer for OpenAPI documents providing a navigation for multiple documents",
  "version": "0.0.1",
  "main": "index.js",
  "author": "Falk Hoppe <falkhoppe81@gmail.com>",
  "license": "Apache-2.0",
  "type": "module",
  "bin": {
    "api-spec-browser": "./bin/api-spec-browser.js"
  },
  "scripts": {
    "build:css": "sass client/css/main.sass dist/css/main.css",
    "watch:css": "npm run build:css && npm run build:css -- --watch",
    "start": "ASB_LOG_LEVEL=info node -r dotenv/config ./bin/api-spec-browser.js --with-examples --config ./examples/example-config.js --cache",
    "build": "npm run build:css",
    "watch:start": "nodemon --ignore node_modules --exec \"npm run start\"",
    "watch": "run-p watch:*"
  },
  "dependencies": {
    "arg": "^4.1.2",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "log4js": "^6.1.2",
    "mobx": "^5.15.4",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "pug": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "redoc": "^2.0.0-rc.29",
    "semver": "^7.1.3",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.26.3"
  }
}
